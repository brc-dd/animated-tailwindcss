{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "animated-tailwindcss",
  "version": "3.3.0",
  "description": "A configuration to use Animate.css with Tailwind CSS",
  "keywords": [
    "animate.css",
    "animatecss",
    "animations",
    "configuration",
    "css",
    "jit",
    "keyframes",
    "plugin",
    "tailwind",
    "tailwindcss",
    "utilities"
  ],
  "homepage": "https://ikcb.org/animated-tailwindcss",
  "bugs": {
    "url": "https://github.com/brc-dd/animated-tailwindcss",
    "email": "brc-dd@hotmail.com"
  },
  "repository": "github:brc-dd/animated-tailwindcss",
  "funding": "https://github.com/sponsors/brc-dd",
  "license": "MIT",
  "author": "Divyansh Singh <brc-dd@hotmail.com> (https://github.com/brc-dd)",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "rimraf dist && tsup ./src/index.ts --dts-resolve --format esm,cjs --minify --treeshake",
    "lint": "lefthook run pre-commit",
    "prerelease": "git add . && pnpm i && pnpm lint && pnpm build",
    "release": "pnpm prerelease && release-it"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@release-it/conventional-changelog": "^5.1.0",
    "@tsconfig/node12": "^1.0.11",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "csstype": "^3.1.1",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.5.0",
    "lefthook": "^1.1.1",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "release-it": "^15.4.2",
    "replace": "^1.2.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.3",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "postcss": "^8",
    "tailwindcss": "^3"
  },
  "peerDependenciesMeta": {
    "postcss": {
      "optional": true
    }
  },
  "packageManager": "pnpm@7.12.2",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}

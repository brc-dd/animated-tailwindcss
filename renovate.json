{
  "extends": ["config:base", ":disableDependencyDashboard", ":maintainLockFilesWeekly"],
  "groupName": "all",
  "ignorePaths": ["**/.yarn/**"],
  "packageRules": [
    {
      "matchUpdateTypes": ["digest", "lockFileMaintenance"],
      "automerge": true,
      "automergeStrategy": "squash"
    }
  ],
  "updatePinnedDependencies": false
}

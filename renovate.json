{
  "automergeStrategy": "squash",
  "dependencyDashboardAutoclose": true,
  "extends": ["config:base"],
  "groupName": "all",
  "ignorePaths": ["**/.yarn/**"],
  "lockFileMaintenance": { "enabled": true },
  "packageRules": [{ "matchUpdateTypes": ["digest", "lockFileMaintenance"], "automerge": true }],
  "updatePinnedDependencies": false
}
